{
  "name": "car-service-book-app",
  "version": "0.0.1",
  "description": "Car service book app made with ❤️ with Next.js",
  "homepage": "https://github.com/teziovsky/car-service-book/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/car-service-book.git"
  },
  "engines": {
    "node": "16.x",
    "npm": ">=v7.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/car-service-book/issues",
    "email": "teziovsky@gmail.com"
  },
  "license": "The Unlicense",
  "author": {
    "name": "teziovsky",
    "email": "teziovsky@gmail.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "private": true,
  "scripts": {
    "clean": "rimraf dist node_modules pnpm-lock.yaml yarn.lock package-lock.json",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "seed": "prisma db seed",
    "dbpush": "prisma db push",
    "dev": "next dev -p 8010",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,scss,md,json}' --loglevel=warn",
    "lint": "npm run prettier && npm run eslint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx,scss,json,md}": "prettier --cache --write",
    "*.{jsx,js,ts,tsx}": "eslint --cache --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "4.14.3",
    "classnames": "2.3.2",
    "daisyui": "2.38.0",
    "formik": "^2.2.9",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "next-superjson": "^0.0.4",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "superjson": "^1.11.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@tanstack/react-query-devtools": "4.14.3",
    "@types/express": "^4.17.14",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "argon2": "^0.30.1",
    "autoprefixer": "10.4.13",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prisma": "^4.5.0",
    "rimraf": "3.0.2",
    "tailwindcss": "3.2.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
